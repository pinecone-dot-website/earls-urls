<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, maximum-scale=1, viewport-fit=cover"
    />
    <meta name="application-name" content="Earls Urls" />
    <link rel="shortlink" href="https://earlsurls.site/" />
    <link rel="stylesheet" href="/static/css/app.css" />
    <link
      rel="apple-touch-icon-precomposed"
      href="/static/apple-touch-icon.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="/static/apple-touch-icon.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="/static/favicon-32x32.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="/static/favicon-16x16.png"
    />
    <link rel="icon" type="image/x-icon" href="/static/favicon.ico" />
    <link rel="manifest" href="/static/site.webmanifest" />

    <title>EARLS URLS</title>
  </head>
  <body class="d-flex flex-column min-vh-100">

    {{#if user.id}}
      <nav class="navbar navbar-expand navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="/">Earls Urls</a>
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link" href="/u/stats">{{user.username}}</a>
            </li>

            <li class="nav-item">
              <a class="nav-link" href="/u/logout">logout</a>
            </li>
          </ul>
        </div>
      </nav>
    {{else}}
      <nav class="navbar navbar-expand-md navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="/">Earls Urls</a>
          <button
            class="navbar-toggler"
            type="button"
            data-toggle="collapse"
            data-target="#toggle"
            aria-controls="toggle"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div
            class="navbar-collapse collapse justify-content-end {{toggle}}"
            id="toggle"
          >
            <form
              method="post"
              action="/u/auth"
              id="login"
              class="navbar-nav align-items-center my-lg-0 flex-column flex-md-row"
              role="form"
            >
              <div class="d-flex flex-row">
                <div class="form-floating my-2 me-2">
                  <input
                    id="username"
                    type="text"
                    class="form-control"
                    name="username"
                    value="{{username}}"
                    placeholder="username"
                  />
                  <label for="username">Username</label>
                </div>

                <div class="form-floating my-2 me-md-2">
                  <input
                    id="password"
                    type="password"
                    class="form-control"
                    name="password"
                    value="{{password}}"
                    placeholder="password"
                  />
                  <label for="password">Password</label>
                </div>
              </div>

              <div class="d-flex">
                <button
                  type="submit"
                  class="btn btn-primary me-2"
                  name="login"
                  value="1"
                >login</button>
                <button
                  type="submit"
                  class="btn btn-primary"
                  name="register"
                  value="1"
                >register</button>
              </div>
            </form>
          </div>
        </div>
      </nav>
    {{/if}}

    <main class="flex-shrink-0">
      <section class="py-5 text-center container">
        {{#if error}}
          <div class="alert alert-warning" role="alert">
            {{error}}
          </div>
        {{/if}}

        <!-- body: -->
        {{{body}}}
      </section>
    </main>

    <div class="container mt-auto">
      <footer class="footer py-3 border-top d-flex justify-content-between">
        <div class="col-md-4 d-flex align-items-center">
          {{#if version}}
            <span class="text-muted">v {{version}}</span>
          {{/if}}
        </div>
        <ul class="nav col-md-4 justify-content-end list-unstyled d-flex">
          <li class="ms-3">
            <a title="Swagger API Docs" class="text-muted" href="/api/docs">
